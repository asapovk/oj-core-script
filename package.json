{
    "name": "oj-core-editor",
    "version": "1.0.0",
    "description": "Editor backend",
    "main": "index",
    "scripts": {
        "fork": "NODE_PATH=node_modules/ pm2 start dist --name oj-core-script",
        "migrate": "node ./scripts/runMigrations.js",
        "start": "node dist/index.js",
        "build": "tsc --p src/tsconfig.json && cp -R ./src/graphql/*.graphql ./dist/graphql &&   cp -R ./src/__boostorm/*.json ./dist/__boostorm",
        "schema": "node scripts/boostorm/schema.js",
        "return": "ts-node scripts/boostorm/return.ts",
        "debug": "ts-node scripts/boostorm/debug.ts",
        "script": "ts-node  --project scripts/tsconfig.json scripts/seed.ts", 
        "codegen": "graphql-codegen --config codegen.ts"
    },
    "bin": "dist/index.js",
    "author": "Konstantin Astapov",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.113.0",
        "@aws-sdk/lib-storage": "^3.113.0",
        "@graphql-codegen/typescript-resolvers": "^1.12.2",
        "@reflexio/reflexio-on-redux": "^0.9.2",
        "@types/nodemailer": "^6.2.0",
        "@types/pg": "^7.14.1",
        "apollo-server": "^2.4.8",
        "apollo-server-express": "^2.4.8",
        "argv-parser": "^0.1.4",
        "aws-sdk": "^2.1157.0",
        "axios": "^0.19.0",
        "bcrypt": "5.0.1",
        "body-parser": "^1.18.2",
        "compression": "^1.7.3",
        "connect-busboy": "^0.0.2",
        "crypto": "^1.0.1",
        "dotenv": "^8.0.0",
        "express": "^4.16.3",
        "express-ws": "^4.0.0",
        "form-data": "^4.0.0",
        "graphql-custom-types": "^1.5.0",
        "graphql-import": "^0.7.1",
        "graphql-iso-date": "^3.6.1",
        "graphql-type-json": "^0.3.0",
        "graphql-upload": "^10.0.0",
        "lodash": "^4.17.20",
        "node-cache-promise": "^1.0.0",
        "node-orangedata": "^1.1.4",
        "nodemailer": "^6.1.1",
        "pg": "^8.11.3",
        "pg-cursor": "2.10.3",
        "pg-promise": "11.5.4",
        "pm2": "^4.5.0",
        "querystring": "^0.2.1",
        "redis": "^2.8.0",
        "redux": "^4.2.1",
        "uuid": "^3.3.3",
        "ws": "^8.1.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/introspection": "4.0.0",
        "@graphql-codegen/typescript": "4.0.1",
        "@graphql-codegen/typescript-resolvers": "4.0.1",
        "@playlyfe/gql": "2.6.2",
        "@pongsatt/graphql-schema-typescript": "^1.0.25",
        "@types/axios": "^0.14.0",
        "@types/compression": "0.0.36",
        "@types/connect-busboy": "^0.0.2",
        "@types/graphql-iso-date": "^3.3.1",
        "@types/graphql-type-json": "^0.1.3",
        "@types/lodash": "^4.14.149",
        "@types/moment": "^2.13.0",
        "@types/node": "^12.0.4",
        "@types/redis": "^2.8.12",
        "@types/uuid": "^3.4.4",
        "graphql": "15.0.0",
        "graphql-binding": "^2.5.1",
        "graphql-code-generator": "^0.18.2",
        "graphql-codegen-typescript-common": "0.18.2",
        "graphql-codegen-typescript-template": "^0.18.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^3.8.0",
        "typescript": "^5.2.2"
    }
}
